{
    "name": "php-code-minifier/php-code-minifier",
    "description": "PHP Code Minifier is a tool that allows you to minify your PHP code.",
    "type": "library",
    "license": ["MIT"],
    "keywords": [
        "php",
        "minifier",
        "minify",
        "code",
        "minification",
        "php-code-minifier"
    ],
    "autoload": {
        "psr-4": {
            "PhpCodeMinifier\\": "src/",
            "PhpCodeMinifier\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Olexandr Mazur",
            "email": "alexandrmazur96@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/console": "^6.0",
        "symfony/polyfill": "^1.33"
    },
    "bin": ["bin/php-code-minifier"],
    "config": {
        "allow-plugins": {
        }
    }
}
